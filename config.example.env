# Luna IoT Server Configuration
# Copy this file to .env and modify the values as needed

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=your_password_here
DB_NAME=luna_iot
DB_SSL_MODE=disable

# Server Ports
HTTP_PORT=8080
TCP_PORT=5000

# Optional: Logging Level (debug, info, warn, error)
LOG_LEVEL=info

# Optional: Maximum number of concurrent TCP connections
MAX_TCP_CONNECTIONS=1000

# SMS Provider (Kaicho Group) Configuration
SMS_API_KEY=your_sms_api_key_here
SMS_API_URL=https://sms.kaichogroup.com/smsapi/index.php
SMS_CAMPAIGN_ID=9148
SMS_ROUTE_ID=130
SMS_SENDER_ID=SMSBit

